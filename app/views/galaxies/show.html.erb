<% @galaxy.suns.each do |sun| %>
  <%= "Name : #{sun.name}" %>
  <%= "Class : #{sun.solar_class}" %>
  <%= "Type : #{sun.solar_type}" %>
  <%= "Chromaticity : #{sun.chromaticity}" %>
  <%= "Ecliptic distance : #{sun.ecliptic_distance}" %>
  <%= "Mass : #{sun.mass}" %>
  <%= "Radius : #{sun.radius}" %>
  <%= "Number of planets : #{sun.planets.count}" %>
  <%= link_to "#{sun.name}", galaxy_sun_path(@galaxy, sun) %>
  <br>
  <br>
<% end %>


